{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".cache",
        "public",
        "dist"
      ]
    },
    "lint": {
      "outputs": [],
      "cache": false
    },
    "types-check": {
      "outputs": [],
      "cache": false
    },
    "test": {
      "outputs": [],
      "cache": false
    },
    "dev": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [],
      "cache": false
    },
    "clean": {
      "outputs": [],
      "cache": false
    },
    "generate-docs": {
      "dependsOn": [
        "usehooks-ts#build"
      ],
      "outputs": [
        "generated"
      ]
    },
    "update-readme": {
      "dependsOn": [
        "usehooks-ts#build"
      ],
      "outputs": [
        "README.md"
      ]
    }
  }
}
