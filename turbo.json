{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist"]
    },
    "lint": {
      "outputs": [],
      "cache": false
    },
    "types-check": {
      "outputs": [],
      "cache": false
    },
    "test": {
      "outputs": [],
      "cache": false
    },
    "dev": {
      "dependsOn": ["^build"],
      "outputs": [],
      "cache": false
    },
    "clean": {
      "outputs": [],
      "cache": false
    },
    "generate-docs": {},
    "update-readme": {}
  }
}
