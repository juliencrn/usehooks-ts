{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist", ".next/**", "!.next/cache/**"]
    },
    "lint": {
      "outputs": [],
      "cache": false
    },
    "types-check": {
      "outputs": [],
      "cache": false
    },
    "test": {
      "outputs": [],
      "cache": false
    },
    "dev": {
      "dependsOn": ["^build"],
      "outputs": [],
      "cache": false
    },
    "clean": {
      "outputs": [],
      "cache": false
    }
  },
  "globalDependencies": ["tsconfig.json"]
}
